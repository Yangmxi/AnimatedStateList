<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:id="@+id/on"
        android:state_activated="true">
        <bitmap android:src="@drawable/p2_walk09" />
    </item>
    <item android:id="@+id/off">
        <bitmap android:src="@drawable/p2_walk03" />
    </item>

    <transition
        android:drawable="@drawable/move_up"
        android:fromId="@+id/on"
        android:toId="@+id/off" >
    </transition>
    <transition
        android:drawable="@drawable/move_down"
        android:fromId="@+id/off"
        android:toId="@+id/on" >
    </transition>

</animated-selector>